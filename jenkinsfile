      //     sh 'ls -ltr'
      //   }
      // }
      stage('Promote to uat') {
        agent any
        steps {
          sh '''
            echo $(git status);
            echo $(git branch -a);
            git checkout uat;
            git merge dev;
          '''
            // git checkout dev;          
        }
      }
    }
  }
